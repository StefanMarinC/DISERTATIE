                                        <hr style="border-top:2px solid #9c301a; margin:0" />
                                    </div>
                                    <div style="padding-top:10px; padding-left:10px; padding-bottom:10px">
                                        <div style="display:flex; justify-content:space-between;  margin-right:10px; margin-bottom:10px">
                                            <div style="font-weight:bold; font-size:large">
                                                Employers
                                            </div>
                                            <button type="button" class="btn btn-primary dropdown-toggle" style="background-color:#9c301a; border:hidden; width:25px; height:25px; display:flex; justify-content:center">
                                                <img src="~/Resources/Add.png" style="width:15px; height:15px;" />
                                            </button>
                                        </div>
                                        @foreach (var subs_employer in Model.subscriberEmployers)
                                        {
                                            if (subs_employer.subscriber_id == subs.subscriber_id)
                                            {
                                                <div style="padding-bottom:5px; margin-right:5px">
                                                    <div style="display:flex; justify-content:space-between; border:1px solid #483c3c; ">
                                                        <div style="display:flex; justify-content:space-between">
                                                            <div style="padding:5px">
                                                                <div style="display:flex">
                                                                    <div>
                                                                        @subs_employer.employer_name
                                                                    </div>
                                                                </div>
                                                                <div style="margin-bottom:5px">
                                                                </div>
                                                                <div style="color:lightslategray; font-size:smaller">
                                                                    @if (subs_employer.main_employer == "1")
                                                                    {
                                                                        <div>
                                                                            Working: Yes
                                                                        </div>
                                                                    }
                                                                    else
                                                                    {
                                                                        <div>
                                                                            Working: No
                                                                        </div>
                                                                    }
                                                                </div>
                                                            </div>
                                                            <div style="display:block; justify-content:end">
                                                            </div>
                                                        </div>
                                                        <div style="padding-top:2px; padding-bottom:2px; padding-right:5px">
                                                            <button type="button" class="btn btn-primary dropdown-toggle" style="background-color:#9c301a; border:hidden; width:25px; height:25px; display:flex; justify-content:center">
                                                                <img src="~/Resources/Edit.png" style="width:15px; height:15px;" />
                                                            </button>
                                                            <div style="margin-bottom:5px">
                                                            </div>
                                                            <button type="button" class="btn btn-primary dropdown-toggle" style="background-color:#9c301a; border:hidden; width:25px; height:25px; display:flex; justify-content:center">
                                                                <img src="~/Resources/Add.png" style="width:15px; height:15px; transform:rotate(45deg)" />
                                                            </button>
                                                        </div>
                                                    </div>
                                                    <div style="width:100%; padding-bottom:5px">
                                                        <hr style="border-top:1px solid black; margin:0" />
                                                    </div>
                                                    <div>
                                                        @subs_employer.employer_name
                                                    </div>
                                                </div>
                                            }
                                        }
                                    </div>
                                    <div style="width:100%">
                                        <hr style="border-top:2px solid #9c301a; margin:0" />
                                    </div>
                                    <div style="padding-top:10px; padding-left:10px; padding-bottom:10px">
                                        <div style="font-weight:bold; font-size:large">
                                            Assets
                                        </div>

                                    </div>
                                </div>
                            }
                        </div>
                    }
                </div>
            </div>
            <div style="width:36%; background-color:white;">
                <div style="background-color:gray; color:white; text-align:center; height:30px; font-size:large">
                    Financial data
                </div>
                <div style="padding-left:10px;">
                    <div style="overflow:auto; max-height:65vh; height:100%;">
                        <div style="margin-right:5px">
                            <div style="font-size:large; padding-bottom:10px; padding-top:10px; font-weight:bold">
                                Account details
                            </div>
                            <div style="display:flex; flex-wrap:wrap; justify-content:start; padding-bottom:10px; width:100%; font-size:14px">
                                <div style="width:25%; margin-bottom:10px">
                                    <div>
                                        Case ID
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.case_id (@Model.FinAccountDetails.account_balance_date.ToString("dd/MM/yyyy"))
                                    </div>
                                </div>
                                <div style="width:25%; margin-bottom:10px">
                                    <div>
                                        Client
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.client_name
                                    </div>
                                </div>
                                <div style="width:25%; margin-bottom:10px">
                                    <div>
                                        Zone
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.zone
                                    </div>
                                </div>
                                <div style="width:25%">
                                    <div>
                                        Customer ID
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.customer_id
                                    </div>
                                </div>
                                <div style="width:25%; margin-bottom:10px">
                                    <div>
                                        Currency
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.account_currency
                                    </div>
                                </div>
                                <div style="width:25%; margin-bottom:10px">
                                    <div>
                                        Amount paid
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.amount_paid @Model.FinAccountDetails.amount_paid_currency
                                    </div>
                                </div>
                                <div style="width:25%; margin-bottom:10px">
                                    <div>
                                        Amount to pay
                                    </div>
                                    <div>
                                        @Model.FinAccountDetails.amount_to_pay @Model.FinAccountDetails.amount_to_pay_currency
                                    </div>
                                </div>
                            </div>
                            <hr style="border-top:2px solid #9c301a; margin:0; padding-bottom:10px;" />
                            <div style="display:flex; justify-content:space-between">
                                <div style="color:black; font-size:large; padding-bottom:10px">
                                    Financial Items
                                </div>
                                <div class="btn-group" style="padding-right:10px;">
                                    <button type="button" class="btn btn-primary dropdown-toggle" data-bs-toggle="dropdown" style="background-color:#9c301a; border:hidden; width:25px; height:25px; display:flex !important; justify-content:center !important">
                                        <img src="~/Resources/Add.png" style="width:15px; height:15px;" />
                                    </button>
                                    <div class="dropdown-menu" style="text-align:center">
                                        <div>
                                            <a id="addPayment" class="dropdown-item" style="text-decoration:none; cursor:pointer; color:black" onclick="@("window.location.href='" + @Url.Action("AddPayment", "Cases", new { case_id = @Model.FinAccountDetails.case_id}) + "'")">Payment</a>
                                        </div>
                                        <div>
                                            <a id="addDiscount" class="dropdown-item" style="text-decoration:none; cursor:pointer; color:black" onclick="@("window.location.href='" + @Url.Action("AddDiscount", "Cases", new { case_id = @Model.FinAccountDetails.case_id}) + "'")">Discount</a>
                                        </div>
                                        <div>
                                            <a id="addCost" class="dropdown-item" style="text-decoration:none; cursor:pointer; color:black">Cost</a>
                                        </div>
                                        <div>
                                            <a id="addDebt" class="dropdown-item" style="text-decoration:none; cursor:pointer; color:black" onclick="@("window.location.href='" + @Url.Action("AddDebt", "Cases", new { case_id = @Model.FinAccountDetails.case_id}) + "'")">Debt</a>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div>
                                <table style="border: 1px solid gray; width: 100%; margin-bottom: 1rem; color: #212529; border-collapse: separate; box-sizing: border-box; display: table; text-indent: initial; border-spacing: 0 10px; background-color:white; font-size:16px">
                                    <thead>
                                        <tr style="border: solid; border-width: 3px 0; ">
                                            <th style="text-align:center">
                                                Item name
                                            </th>
                                            <th style="text-align:center">
                                                Item date
                                            </th>
                                            <th style="text-align:center">
                                                Booked
                                            </th>
                                            <th style="text-align:center">
                                                Amount
                                            </th>
                                            <th style="text-align:center">
                                                Not allocated
                                            </th>
                                            <th style="text-align:center">
                                                Actions
                                            </th>
                                        </tr>
                                        <tr>
                                            <td colspan="6">
                                                <hr style="border: 1px solid black; margin:0" />
                                            </td>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var FinItem in Model.financialItems)
                                        {
                                            <tr style="border-bottom: 1px solid #000;">
                                                <td style="text-align:center">
                                                    @FinItem.item_name
                                                    @if (FinItem.amount_over > 0)
                                                    {
                                                        <small _ngcontent-iqe-c273="" data-toggle="tooltip" data-placement="top" class="badge badge-primary mr-1 ng-star-inserted" title="Overpayment">! </small>
                                                    }
                                                </td>
                                                <td style="text-align:center">
                                                    @FinItem.item_date
                                                </td>
                                                <td style="text-align:center">
                                                    @FinItem.booking_date
                                                </td>
                                                @if (FinItem.sign < 0)
                                                {
                                                    <td style="text-align:center; color:red">
                                                        - @FinItem.amount @FinItem.amount_currency
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td style="text-align:center; color:mediumseagreen">
                                                        + @FinItem.amount @FinItem.amount_currency
                                                    </td>
                                                }
                                                @if (FinItem.sign < 0)
                                                {
                                                    <td style="text-align:center; color:red">
                                                        - @FinItem.amount_not_booked @FinItem.amount_not_booked_currency
                                                    </td>
                                                }
                                                else
                                                {
                                                    <td style="text-align:center; color:mediumseagreen">
                                                        + @FinItem.amount_not_booked @FinItem.amount_not_booked_currency
                                                    </td>
                                                }
                                                @if (FinItem.item_name != "Interest")
                                                {
                                                    <td>
                                                        <div style="display:flex; justify-content:center">
                                                            <button type="button" class="btn btn-primary dropdown-toggle" style="background-color:#9c301a; border:hidden; width:30px; height:30px; display:flex !important; justify-content:center !important">
                                                                <img src="~/Resources/Storno.png" style="width:15px; height:15px;" />
                                                            </button>
                                                        </div>
                                                        <div style="margin-bottom:5px">
                                                        </div>
                                                        <div style="display:flex; justify-content:center;">
                                                            <button type="button" class="btn btn-primary dropdown-toggle" style="background-color:#9c301a; border:hidden; width:30px; height:30px; display:flex !important; justify-content:center !important;">
                                                                <img src="~/Resources/Info.png" style="width:20px; height:20px;" />
                                                            </button>
                                                        </div>

                                                    </td>
                                                }
                                            </tr>
                                            <tr>
                                                <td colspan="6">
                                                    <hr style="border: 1px solid gray; margin:0" />
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>


            </div>
            <div style="width:30%; background-color:white">
                <div style="display:flex; justify-content:flex-start; background-color:gray">
                    <button id="select_comments" name="button_info" style=" background-color: #9c301a ; color:white; height:30px; border:hidden" onclick="change_info('comments')">
                        Comments
                    </button>
                    <button id="select_letters" name="button_info" style=" background-color: gray ; color:white; height:30px; border:hidden" onclick="change_info('letters')">
                        Letters
                    </button>
                    <button id="select_emails" name="button_info" style=" background-color: gray ; color:white; height:30px; border:hidden" onclick="change_info('emails')">
                        Emails
                    </button>
                    <button id="select_sms" name="button_info" style=" background-color: gray ; color:white; height:30px; border:hidden" onclick="change_info('sms')">
                        SMS
                    </button>
                </div>
                <div>
                    <div id="comments" name="div_info" style="display:block; padding-bottom:5px">
                        comments
                    </div>
                    <div id="letters" name="div_info" style="display:none; padding-bottom:5px">
                        letters
                    </div>
                    <div id="emails" name="div_info" style="display:none; padding-bottom:5px">
                        emails
                    </div>
                    <div id="sms" name="div_info" style="display:none; padding-bottom:5px">
                        sms
                    </div>
                </div>
            </div>
        </div>
    </div>

    <script>
        function change_subs(value) {
            var elements = document.getElementsByName("div_subs");
            for (var i = 0; i < elements.length; i++) {
                elements[i].style.display = "none";
            }
            elements = document.getElementsByName("select_subs");
            for (var i = 0; i < elements.length; i++) {
                elements[i].style.backgroundColor = "gray";
            }
            document.getElementById("div_" + value).style.display = "block";
            document.getElementById("select_subs_" + value).style.backgroundColor = "#9c301a";
        }
    </script>

    <script>
        function change_info(value) {
            var elements = document.getElementsByName("div_info");
            for (var i = 0; i < elements.length; i++) {
                elements[i].style.display = "none";
            }
            elements = document.getElementsByName("button_info");
            for (var i = 0; i < elements.length; i++) {
                elements[i].style.backgroundColor = "gray";
            }
            document.getElementById(value).style.display = "block";
            document.getElementById("select_" + value).style.backgroundColor = "#9c301a";
        }
    </script>

    <script>
        function edit_subs(case_id) {
            var subs_element;
            var elements = document.getElementsByName("div_subs");
            for (var i = 0; i < elements.length; i++) {
                if (elements[i].style.display == "block") {
                    subs_element = elements[i];
                }
            }
            var subs_id = subs_element.id;
            var url = '/Cases/EditPerson?case_id=' + case_id + '&subscriber_id=' + subs_id;
            window.location.href = url;
        }
    </script>

    <script>
        function delete_subs(case_id) {
            var subs_element;
            var elements = document.getElementsByName("div_subs");
            for (var i = 0; i < elements.length; i++) {
                if (elements[i].style.display == "block") {
                    subs_element = elements[i];
                }
            }
            var subs_id = subs_element.id;
            var url = '/Cases/DeletePerson?case_id=' + case_id + '&subscriber_id=' + subs_id;
            window.location.href = url;
        }
    </script>

</body>
</html>
